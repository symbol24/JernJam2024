; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="In Death We Trust"
run/main_scene="res://Scenes/Levels/boot.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Signals="*res://Code/Autoloads/signals.gd"
Audio="*res://addons/AudioBySymbol/Code/Autoloads/audio_autoload.gd"
DataManager="*res://Scenes/Utilities/data_manager.tscn"
Game="*res://Code/Autoloads/game.gd"
UI="*res://Scenes/UI/UI.tscn"

[debug]

gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/AudioBySymbol/plugin.cfg")

[file_customization]

folder_colors={
"res://Audio/": "blue",
"res://Code/": "yellow",
"res://Data/": "purple",
"res://Scenes/": "red",
"res://Textures/": "green"
}

[global_group]

enemy=""
spawn_point=""
room=""
camera_point=""
generator=""

[gui]

theme/custom="res://Data/default_theme.tres"
theme/custom_font="res://Data/NotJamPixel5.ttf"

